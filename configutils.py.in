def parseConf():
    ret = {}
    with open("/etc/@package_name@conf/@package_name@.conf", "r") as configfile:
        for line in configfile.readlines():
            line = line.strip()
            toks = line.split(":")
            if len(toks) == 2:
                ret[toks[0].strip()] = toks[1].strip()
        configfile.close()
    return ret
    
if __name__ == '__main__':
	print(parseConf())
